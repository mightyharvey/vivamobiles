{
  "components/common/pagination": "<div class=\"pagination-container{{#if is_grid}} layout-grid-item{{/if}}\">\n  <ul class=\"pagination-list\">\n\n    {{#if previous}}\n      <li class=\"pagination-item previous\">\n        <a class=\"pagination-link pagination-item--previous\" href=\"{{previous}}\">\n          <i class=\"icon-arrow-left\"></i>\n          {{lang 'common.previous'}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#each links}}\n      <li class=\"pagination-item\">\n        <a class=\"pagination-link {{#if ../current \"==\" number}}active{{/if}}\" href=\"{{url}}\" data-pagination-current-page-link\n            >\n            {{number}}\n        </a>\n      </li>\n    {{/each}}\n\n    {{#if next}}\n      <li class=\"pagination-item next\">\n        <a class=\"pagination-link pagination-item--next\" href=\"{{next}}\">\n          <i class=\"icon-arrow-right \"></i>\n          {{lang 'common.next'}}\n        </a>\n      </li>\n    {{/if}}\n\n  </ul>\n</div>\n"
}
